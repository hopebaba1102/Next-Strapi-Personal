  "license": "MIT",
  "browser": {
    "fs": false,
    "path": false,
    "os": false
  },
  "scripts": {
    "dev": "next",
    "start": "next start",
    "build": "next build && npm run generate-manifest",
    "deploy": "npm run build && npm run start",
    "export": "next export",
    "generate-manifest": "node ./lib/manifest.js",
    "img-optimize": "py ./utils/resize_images.py",
    "lint": "eslint --fix .",
    "format": "prettier --write './**/*.{js,jsx,css,json}' --config ./.prettierrc",
    "count": "cloc --exclude-ext=svg --exclude-dir=animations,node_modules,.next --json components/ lib/ pages/ styles/ public/ . > data/stats/count_total.json"
  },
  "dependencies": {
    "@socialgouv/matomo-next": "^1.1.2",
