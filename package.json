    "export": "next export",
    "generate-manifest": "node ./lib/manifest.js",
    "img-optimize": "py ./utils/resize_images.py",
    "lint": "eslint --fix .",
    "format": "prettier --write './**/*.{js,jsx,css,json}' --config .prettierrc",
    "count": "cloc --exclude-ext=svg --exclude-dir=animation-data,node_modules,.next --json components/ lib/ pages/ styles/ public/ . > lib/data/count_total.json"
  },
