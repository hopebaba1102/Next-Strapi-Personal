    "leaflet": "^1.7.1",
    "next": "^10.0.5",
    "next-compose-plugins": "^2.2.1",
    "next-mdx-remote": "^2.1.3",
    "next-pwa": "^5.0.4",
    "object-hash": "^2.1.1",
