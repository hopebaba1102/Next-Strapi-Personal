    "cloc": "^2.7.0",
    "d3": "^6.5.0",
    "date-fns": "2.16.1",
    "globby": "^11.0.2",
    "next": "^10.0.5",
    "next-compose-plugins": "^2.2.1",
    "next-pwa": "^5.0.4",
    "polished": "^4.1.0",
    "prettier": "1.16.4",
    "prismjs": "^1.23.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-emoji-render": "^1.2.4",
    "react-leaflet": "^3.0.5",
    "react-lottie": "^1.2.3",
